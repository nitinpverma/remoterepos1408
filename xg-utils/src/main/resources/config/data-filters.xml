<?xml version="1.0" encoding="UTF-8"?>
<!--Duplicate file (xgws-web\src\main\webapp\WEB-INF\config) for spring beans loading from this project-->
<beans xmlns="http://www.springframework.org/schema/beans" 
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://www.springframework.org/schema/beans 
							http://www.springframework.org/schema/beans/spring-beans.xsd" >

	<bean id="contractAddDataFilter" class="com.csc.fsg.life.xg.ws.datafilters.HoldingDataFilter">
		<property name="allowedElemsMap">
			<map>
				<entry key="Holding">
					<list>
						<value>HoldingTypeCode</value>
						<value>HoldingStatus</value>
						<value>Policy</value>
					</list>
				</entry>
				<entry key="Policy">
					<list>
						<value>PolNumber</value>
						<value>ProductType</value>
						<value>EffDate</value>
						<value>ReplacementType</value>
						<value>OLifEExtension</value>
					</list>
				</entry>
				<entry key="OLifEExtension">
					<list>
						<value>PolicyExtension</value>
					</list>
				</entry>
				<entry key="PolicyExtension">
					<list>
						<value>YearIssued</value>
					</list>
				</entry>
			</map>
		</property>
	</bean>
</beans>
